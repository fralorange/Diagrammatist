<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="TitleBarBackground" Color="{DynamicResource TitleBarBG}" />
    <SolidColorBrush x:Key="TitleBarButtonBackground" Color="{DynamicResource TitleBarButtonBG}" />
    <SolidColorBrush x:Key="TitleBarButtonForeground" Color="{DynamicResource TitleBarButtonFG}" />
    <SolidColorBrush x:Key="TitleBarButtonHoverBackground" Color="{DynamicResource TitleBarButtonHoverBG}" />
    <SolidColorBrush x:Key="TitleBarButtonHoverForeground" Color="{DynamicResource TitleBarButtonFG}" />
    <SolidColorBrush x:Key="TitleBarButtonPressedBackground" Color="{DynamicResource TitleBarButtonPressedBG}" />
    <SolidColorBrush x:Key="TitleBarButtonPressedForeground" Color="{DynamicResource TitleBarButtonFG}" />

    <SolidColorBrush x:Key="TitleBarCloseButtonBackground" Color="{DynamicResource TitleBarCloseButtonBG}" />
    <SolidColorBrush x:Key="TitleBarCloseButtonForeground" Color="{DynamicResource TitleBarCloseButtonFG}" />
    <SolidColorBrush x:Key="TitleBarCloseButtonHoverBackground" Color="{DynamicResource TitleBarCloseButtonHoverBG}" />
    <SolidColorBrush x:Key="TitleBarCloseButtonHoverForeground" Color="{DynamicResource TitleBarCloseButtonHoverPressedFG}" />
    <SolidColorBrush x:Key="TitleBarCloseButtonPressedBackground" Color="{DynamicResource TitleBarCloseButtonPressedFG}" />
    <SolidColorBrush x:Key="TitleBarCloseButtonPressedForeground" Color="{DynamicResource TitleBarCloseButtonHoverPressedFG}" />

    <SolidColorBrush x:Key="ButtonHoverBackground" Color="{DynamicResource ButtonHoverBG}"/>
    <SolidColorBrush x:Key="ButtonPressedBackground" Color="{DynamicResource ButtonPressedBG}"/>

    <SolidColorBrush x:Key="FrameStroke" Color="{DynamicResource AppThemeColor}"/>
    <SolidColorBrush x:Key="FrameBackground" Color="{DynamicResource AppBackground}"/>

    <SolidColorBrush x:Key="WindowBackground" Color="{DynamicResource AppSecondaryBackground}"/>

    <SolidColorBrush x:Key="DialogFrameStroke" Color="{DynamicResource AppDialogStroke}"/>

    <SolidColorBrush x:Key="ShapeDefaultStroke" Color="{DynamicResource AppThemeColor}"/>
    <SolidColorBrush x:Key="ShapeDefaultBackground" Color="{DynamicResource AppBackground}"/>

    <SolidColorBrush x:Key="ToolbarBackground" Color="Transparent"/>

    <SolidColorBrush x:Key="SettingsTabsBackground" Color="{DynamicResource DialogSecondaryBackground}"/>
</ResourceDictionary>